<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.3"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Benoit Courtine"
    RELEASE_DATE="20090506A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>
  
  <FAMILIES>
	<FAMILY>File/Management</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="10"
    >
      <TABLE READONLY="true">
		<COLUMN NAME="filename" TYPE="id_String" LENGTH="255" />
		<COLUMN NAME="digestHexaString" TYPE="id_String" LENGTH="255" />
		<COLUMN NAME="digestBytes" TYPE="id_byte[]" />
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/file.txt"</DEFAULT>
    </PARAMETER>

	<PARAMETER 
	  NAME="DIGEST_METHOD"
	  FIELD="CLOSED_LIST"
	  NUM_ROW="30"
	  REQUIRED="true"
	>
    	<ITEMS DEFAULT="MD5">
			<ITEM NAME="MD2" VALUE="MD2" />
			<ITEM NAME="MD5" VALUE="MD5" />
			<ITEM NAME="SHA-1" VALUE="SHA-1" />
			<ITEM NAME="SHA-256" VALUE="SHA-256" />
			<ITEM NAME="SHA-384" VALUE="SHA-384" />
			<ITEM NAME="SHA-512" VALUE="SHA-512" />
    	</ITEMS>
    </PARAMETER>
	
    <PARAMETER
      NAME="BUFFER_SIZE"
      FIELD="TEXT"
      NUM_ROW="40"
      REQUIRED="true"
    >
    	<DEFAULT>1024</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
  <CODEGENERATION/>
  
  <RETURNS/>
  
</COMPONENT>