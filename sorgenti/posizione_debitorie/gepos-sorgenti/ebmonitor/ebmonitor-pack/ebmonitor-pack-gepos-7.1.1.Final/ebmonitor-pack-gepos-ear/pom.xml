<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>ebmonitor-pack-gepos-7.1.1.Final</artifactId>
    <groupId>it.tasgroup.idp</groupId>
    <version>1.0</version>
  </parent>
  <artifactId>ebmonitor-pack-gepos-ear</artifactId>
  <packaging>ear</packaging>
  <name>- ear - gepos payflowpa</name>
  <description>gepos payflowpa ear project</description>
  <version>${releaseVersion}</version>
  <properties>
    <earName>gepos-gpd-${releaseVersion}</earName>
    <ear.release.folder>${be.release.folder}/gepos</ear.release.folder>
  </properties>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-ear-plugin</artifactId>
        <version>2.6</version>
        <configuration>
          <version>6</version>
          <displayName>${earName}</displayName>
          <finalName>${earName}</finalName>
          <defaultLibBundleDir>lib</defaultLibBundleDir>
          <earName>${earName}</earName>
          <outputDirectory>${ear.release.folder}</outputDirectory>
          <applicationName>EBM_eBMonitor</applicationName>
          <modules>
            <!-- EJBs -->
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpJBossStartup-7.1.1.Final</artifactId>
              <bundleFileName>IdpJBossStartup-7.1.1.Final.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpEjbCommon</artifactId>
              <bundleFileName>IdpEjbCommon.jar</bundleFileName>
            </ejbModule>
 
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>GestoreEventi</artifactId>
              <bundleFileName>GestoreEventi.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>DRV_CART</artifactId>
              <bundleFileName>DRV_CART.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpCbill</artifactId>
              <bundleFileName>IdpCbill.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>DRV_PROXY_NDP</artifactId>
              <bundleFileName>DRV_PROXY_NDP.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpProperties</artifactId>
              <bundleFileName>IdpProperties.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpBillerServicesCore</artifactId>
              <bundleFileName>IdpBillerServicesCore.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpBillerNdpServicesCore</artifactId>
              <bundleFileName>IdpBillerNdpServicesCore.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpCartJms</artifactId>
              <bundleFileName>IdpCartJms.jar</bundleFileName>
            </ejbModule>
            <ejbModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpCartEjb</artifactId>
              <bundleFileName>IdpCartEjb.jar</bundleFileName>
            </ejbModule>
            <!-- WEB -->
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpAdmin</artifactId>
              <bundleFileName>IdpAdmin.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpWebServices</artifactId>
              <bundleFileName>IdpWebServices.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpBillerServices</artifactId>
              <bundleFileName>IdpBillerServices.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpCartWebServices</artifactId>
              <bundleFileName>IdpCartWebServices.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpBillerNdpServices</artifactId>
              <bundleFileName>IdpBillerNdpServices.war</bundleFileName>
            </webModule>
            <webModule>
              <groupId>it.tasgroup.idp</groupId>
              <artifactId>IdpBillerNdpServicesClient</artifactId>
              <bundleFileName>IdpBillerNdpServicesClient.war</bundleFileName>
            </webModule>
          </modules>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>ebmonitor-pack-gepos-conf</artifactId>
      <version>${releaseVersion}</version>
    </dependency>
    <!-- solo per aggiungere il plugin alla lib dell'ear -->
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpPluginExtensions</artifactId>
    </dependency>
    <!-- ejbs -->
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpJBossStartup-7.1.1.Final</artifactId>
      <type>ejb</type>
    </dependency>

    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpEjbCommon</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>GestoreEventi</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>DRV_CART</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpCbill</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>DRV_PROXY_NDP</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpProperties</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpBillerServicesCore</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpBillerNdpServicesCore</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpCartEjb</artifactId>
      <type>ejb</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpCartJms</artifactId>
      <type>ejb</type>
    </dependency>
    <!-- wars -->
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpAdmin</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpWebServices</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpBillerServices</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpCartWebServices</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpBillerNdpServices</artifactId>
      <type>war</type>
    </dependency>
    <dependency>
      <groupId>it.tasgroup.idp</groupId>
      <artifactId>IdpBillerNdpServicesClient</artifactId>
      <type>war</type>
    </dependency>
  </dependencies>
</project>
