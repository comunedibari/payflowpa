<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="countConverter" class="it.nch.streaming.services.impl.TotalLineNumberPrevisitToFlatConversion" singleton="false">
		<constructor-arg>
			<ref local="converter"/>
		</constructor-arg>
	</bean>
	
	<bean id="converter" class="it.nch.streaming.services.impl.exp.UbiPmtreqToFlatConversionServiceNew" singleton="false">
